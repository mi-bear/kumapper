{
  "name": "development",
  "domain": "http://localhost:3000",
  "gtm": {
    "containerId": ""
  },
  "log4js": {
    "appenders": [
      {
        "category": "access",
        "type": "dateFile",
        "filename": "logs/access.log",
        "pattern": "-yyyy-MM-dd",
        "maxLogfSize":20480,
        "backups": 10
      },
      {
        "category": "system",
        "type": "dateFile",
        "filename": "logs/system.log",
        "pattern": "-yyyy-MM-dd",
        "maxLogfSize":20480,
        "backups": 10
      },
      {
        "category": "error",
        "type": "dateFile",
        "filename": "logs/error.log",
        "pattern": "-yyyy-MM-dd",
        "maxLogfSize":20480,
        "backups": 10
      },
      {
        "type": "console"
      }
    ],
    "levels": {
      "access": "ALL",
      "system": "ALL",
      "error": "ALL"
    },
    "replaceConsole": true
  }
}
